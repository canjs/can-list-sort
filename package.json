  },
  "devDependencies": {
    "can-compute": "^3.0.1",
    "can-model": "^3.0.0",
    "can-stache": "^4.5.0",
    "jquery": "^3.1.1",
    "jshint": "^2.9.1",
    "steal": "^0.16.0",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.16.0",
    "testee": "^0.7.0"
  }
